package üs.alma;
import java.util.Scanner;
public class ÜsAlma {

    public static void main(String[] args) {
     
        int taban, us,total=1;
        Scanner inp=new Scanner(System.in);
      
        System.out.print("Tabanı giriniz: ");
        taban=inp.nextInt();
        System.out.print("Üssü giriniz: ");
        us=inp.nextInt();
        
        for(int n=1;n<=us;n++){
        total*=taban;
        }
        System.out.println(total);
    }  
}
